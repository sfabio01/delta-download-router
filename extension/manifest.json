{
    "manifest_version": 3,
    "name": "Delta",
    "version": "0.1.0",
    "description": "The most sofisticated download manager for your browser",
    "icons": {
        "128": "icons/icon128.png"
    },
    "options_page": "options.html",
    "permissions": [
        "downloads",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    }
}